version 16
__lua__
frame = 0
anim  = 0
sz  = 23
ai  = {}
obs = {}
fa  = {}
fas = 2
winner = 0
victory = false
menu = true

--world physics
ori = {}
grip  = 0.98
force = 0.03

function _init()
cls()

fa[0] = faction(0,8,2)
fa[1] = faction(1,12,1)
fa[2] = faction(2,3)

ori = vec:new(64,64)
obs = obst:new(64,64)

 for i=0,sz do
 	ai[i] = agent:new(
 	i,
 	rnd()*64+64*(i%2),
 	rnd()*128,
 	fa[i%fas])
 	fa[i%fas].hps += ai[i].hp
 end
 
end

function reset()

	for i=0,fas do
		fa[i].dead = 0
		fa[i].pts = 0
		fa[i].hps = 0
	end
	
 for i=0,sz do
 	ai[i]:reset(
 	i,
 	rnd()*64+64*(i%2),
 	rnd()*128,
 	fa[i%fas])
 	fa[i%fas].hps += ai[i].hp
 end
 
end

function _update()
if menu == false then
	for i=0,sz do
	for t=0,sz do
 	if i != t then
  	ai[i]:think(ai[t])
  end
	end
 end
  
  d = {}
  d[0] = 0
  d[1] = 0
  
 for i=0,sz do
 	ai[i]:update()
 	if ai[i].hp < 1 then
 	d[ai[i].fa] += 1
 	end
 end
 
 for i=0,fas do
 	fa[i].dead = d[i]
 	if fa[i].dead == 12
 	 and victory == false then
  	fa[i] = faction(i)
  	fa[(i+1)%fas].win += 1
  	winner = fa[(i+1)%fas].id
			victory = true
 	end
 end
 
 if btn(4) and victory then
 	reset()
 	victory = false
 end
else

 if btn(4) and menu then
 	reset()
 	menu = false
 end

end
end

function _draw()
 cls()
 if menu==false then
 
 candy:draw()
	--obs:draw()
 
	for i=0,sz do
 ai[i]:draw()
 end
 
	if victory then
		print("🐱 wins",0,0,fa[winner].c1)
		print("press z to load new opponent",0,8,1)	
	else
 print("∧",0,0,1)
 print("⬇️",24,0,1)
	print("◆",48,0,1)
	print("♥",72,0,1)
	for i=0,fas-1 do
 print(fa[i].win,0,8*(i+1),fa[i].c1)
	print(fa[i].dead,24,8*(i+1),fa[i].c1)
	print(fa[i].pts,48,8*(i+1),fa[i].c1)
	print(fa[i].hps,72,8*(i+1),fa[i].c1)
	end
 end
 else
 	print("conflict simulator",0,0,8)
		print("press z to load new opponent",0,8,1)	
 	
 end
end
