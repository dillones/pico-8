-- veclib
vec = {}
vec_mt = {__index = vec}
vec.x = 0
vec.y = 0

function vec:new(x,y)
	local inst = {}
	inst.x = x
	inst.y = y
	setmetatable(inst, self)
	self.__index = self
	
	return inst
end

function vec:set(x,y)
	self.x = x
	self.y = y
end

function vec:copy(v)
		local inst = {}
 	inst.x = v.x
 	inst.y = v.y
 	setmetatable(inst, self)
 	self.__index = self
 	
 	return inst
end

function vec:norm()
	if  (abs(self.x) > 0)
	and (abs(self.y) > 0) then
 	a = v.x^2
 	b = v.y^2
 	c = sqrt(a+b)
 	self:div(c)
	end
end

function vec:limit(n)
	if self:length() > n then
	self:norm()
	self:mult(n)
	end
end

function vec:length()
	if  (self.x != 0)
	and (self.y != 0) then
 	a = v.x^2
 	b = v.y^2
 	c = sqrt(a+b)
 	return c
	end
end

function vec:add(v)
 self.x+=v.x
 self.y+=v.y
end

function vec:dist(v)
	c = 0
	t1 = self.x-v.x
	t2 = self.y-v.y
	
	if  (t1 != 0)
	and (t2 != 0)
	then
	a = (t1)^2
	b = (t2)^2
	c = sqrt((a+b))
	end
	return c
end

function vec:sub(v)
	self.x-=v.x
	self.y-=v.y
end

function vec:mult(n)
	self.x *= n
 self.y *= n
end

function vec:div(n)
	self.x /= n
	self.y /= n
end
