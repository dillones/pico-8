--faction generator
function faction(id)
s = {}

pl = {}
pl[0] = {a=8,d=2}
pl[1] = {a=14,d=2}
pl[2] = {a=12,d=1}
pl[3] = {a=11,d=3}
pl[4] = {a=10,d=9}

s.id = id
s.pid=flr(rnd(5))

for f in all(fa) do
  while s.pid == f.pid do
  s.pid += 1
  s.pid %= 5
  end
end

s.c1 = pl[s.pid].a
s.c2 = pl[s.pid].d

s.sep_d = 4+rnd()*5
s.sep_m = 5+rnd()*5

s.ali_d = s.sep_d+rnd()*20
s.ali_m = 1+rnd()*1

s.coh_d = s.sep_d+rnd()*15
s.coh_m = rnd()*1

s.force = force
s.forage = 10+rnd()*10

s.fight_d = 10+rnd()*20
s.fight_m = rnd()*5+2

s.avoid_m = 10+rnd()*20*-1

s.hp    = 20
s.grip  = grip
s.force = force

s.dead = 0
s.win  = 0
s.pts = 0
s.hps = 0

return s
end
