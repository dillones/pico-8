-- agent
agent = {}
agent.id = 0
agent.loc = {}
agent.vel = {}
agent.acc = {}
agent.x = 0
agent.y = 0
agent.fa = 0
agent.hp = 0
agent.c1 = 0
agent.c2 = 0

--stats
agent.sep_d = 0
agent.sep_m = 0

agent.ali_d = 0
agent.ali_m = 0

agent.coh_d = 0
agent.coh_m = 0

agent.fight_d = 0
agent.fight_m = 0

agent.avoid_m = 0

agent.force = 0
agent.grip = .99

agent.seq = 0
agent.tmr = 24

function agent:new(id,x,y,f)

	loc = vec:new(x,y)
	vel = vec:new(0,0)
	acc = vec:new(0,0)

	local inst = {}
	inst.id  = id
 inst.loc = loc
 inst.vel = vel
 inst.acc = acc
 
 inst.sep_d = f.sep_d
 inst.sep_m = f.sep_m
 
 inst.ali_d = f.ali_d
 inst.ali_m = f.ali_m
 
 inst.coh_d = f.coh_d
 inst.coh_m = f.coh_m
 
 inst.frg_m = f.forage
 inst.force = f.force
 
 inst.fight_d = f.fight_d
 inst.fight_m = f.fight_m

	inst.avoid_m = f.avoid_m

	inst.fa = f.id
	inst.hp = 20
	inst.grip = f.grip
	
	inst.seq = 0
	inst.tmr = 24
	
	inst.c1 = f.c1
	inst.c2 = f.c2
	
	setmetatable(inst, self)
	self.__index = self
	
	return inst
	end
	
function agent:reset(id,x,y,f)
	s = self
 s.loc:set(x,y)
 s.vel:set(0,0)
 s.acc:set(0,0)
 
 s.sep_d = f.sep_d
 s.sep_m = f.sep_m
 
 s.ali_d = f.ali_d
 s.ali_m = f.ali_m
 
 s.coh_d = f.coh_d
 s.coh_m = f.coh_m
 
 s.frg_m = f.forage
 s.force = f.force
 
 s.fight_d = f.fight_d
 s.fight_m = f.fight_m

	s.avoid_m = f.avoid_m

	s.force = f.force
	s.grip = f.grip
	s.hp = f.hp
	
	s.tmr = 24
	
	s.c1 = f.c1
	s.c2 = f.c2
	
end

function agent:update()
 s = self

	if s.hp > 0 then

 	f = s:forage()
 	s.acc:add(f)
 	
 	//s:avoid(obs,s)
 	
 	s.acc:norm()
 	s.acc:mult(s.force)
 	s.vel:add(s.acc)
 	
 	s.vel:mult(s.grip)
 	s.loc:add(s.vel)
 	
 	c_dist = s.loc:dist(candy)
 	if c_dist < 6 then
  	candy:spawn(s.fa)
  	s.hp += 2
  	fa[s.fa].pts +=2
  	fa[s.fa].hps +=2
 	end
 	end
 
 if s.tmr == 24
 and s.hp == 0 then
 sfx(2,3,0,7)
 end
 	
end

function agent:draw()
	s = self
	if(s.hp > 0) then
	circ(s.loc.x,
						s.loc.y,
						3,s.c1)					
	pset(s.loc.x,s.loc.y,s.c1)
	
	else if s.tmr != 0 then
	circ(s.loc.x,
						s.loc.y,
						3+4-s.tmr/6,s.c2)
	circ(s.loc.x,
						s.loc.y,
						0+3*((24-s.tmr)/3),
						s.c1)
	
	s.tmr-=1
	end
	end
end

function agent:think(ai)
		s = self
 	if s.fa == ai.fa and ai.hp > 0 then
 	s:flock(ai)
 	else if ai.hp > 0 then
 	s:fight(ai)
 	end
 	end
end

function agent:flock(t)
	s = self
	d = s.loc:dist(t.loc)
	
	if d < s.sep_d
	then
 	v = s:sep(t.loc)
 	v:mult(s.sep_m)
 	s.acc:add(v)
 end
 if d < s.ali_d then
 	v = s:ali(t.vel)
 	v:mult(s.ali_m)
 	s.acc:add(v)
	end
	if  d < s.coh_d
	and d > s.sep_d then
 	v = s:coh(t.loc)
 	v:mult(s.coh_m)
 	self.acc:add(v)
	end

end

function agent:fight(t)
	s = self
	if s.hp/t.hp > 1.5 then agg = -1
	else agg = 1 end
	
	d = s.loc:dist(t.loc)
 	if d < s.fight_d then
 	v = s:sep(t.loc)
 	v:mult(s.fight_m*agg)
 	s.acc:add(v)
	end
	if d < 6
	and s.hp > 0 then
		s.vel:mult(0.8)
 	s.hp -= 1
 	fa[s.fa].hps -=1
 	sfx(1,1,0,4)
	end
end

function agent:sep(t)
		v = vec:new(self.loc.x,self.loc.y)
		v:sub(t)
		v:norm()
		return(v)
end

function agent:ali(t)
		v = vec:new(t.x,t.y)
		v:norm()
		return(v)
end

function agent:coh(t)
		v = vec:new(t.x, t.y)
		v:sub(self.loc)
		v:norm()
		return(v)
end

function agent:forage()
		v = vec:new(candy.x, candy.y)
		v:sub(self.loc)
		v:norm()
		v:mult(self.frg_m)
		return(v)
end
